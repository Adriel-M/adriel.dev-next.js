{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "dev:with-tina": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"cross-env INIT_CWD=$PWD next dev\"",
    "build": "yarn run build:conditionaltinacms && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "build:tinacms": "tinacms build",
    "build:conditionaltinacms": "bash scripts/build-tinacms.sh",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@next/bundle-analyzer": "14.1.3",
    "@primer/octicons": "^19.8.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/speed-insights": "^1.0.9",
    "autoprefixer": "^10.4.13",
    "compromise": "^14.11.0",
    "contentlayer": "0.3.4",
    "feed": "^4.2.2",
    "github-slugger": "^2.0.0",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "next": "14.1.3",
    "next-contentlayer": "0.3.4",
    "pliny": "0.1.7",
    "postcss": "^8.4.24",
    "probe-image-size": "^7.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-obfuscate-email": "1.1.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^1.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.0",
    "rehype-slug": "^6.0.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.33.2",
    "strip-markdown": "^6.0.0",
    "tailwindcss": "^3.4.1",
    "tinacms": "^1.5.28"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@tinacms/cli": "^1.5.39",
    "@types/mdx": "^2.0.5",
    "@types/node": "^20.10.6",
    "@types/primer__octicons": "^19",
    "@types/probe-image-size": "^7",
    "@types/react": "^18.2.14",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.45.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.1.1"
}
